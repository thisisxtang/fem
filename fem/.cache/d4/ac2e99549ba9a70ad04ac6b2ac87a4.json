ted":{"line":110,"column":0},"source":"Details.js","original":{"line":64,"column":3}},{"name":"Component","generated":{"line":114,"column":2},"source":"Details.js","original":{"line":11,"column":22}},{"name":"Details","generated":{"line":116,"column":15},"source":"Details.js","original":{"line":67,"column":15}}],"sources":{"Details.js":"import React, { Component } from \"react\";\nimport pf from \"petfinder-client\";\nimport { navigate } from \"@reach/router/lib/history\";\nimport Carousel from \"./Carousel\";\n\nconst petfinder = pf({\n  key: process.env.API_KEY,\n  secret: process.env.API_SECRET\n}); // refer things from env file\n\nclass Details extends Component {\n  constructor(props) {\n    super(props);\n\n    this.state = {\n      loading: true\n    };\n  }\n\n  componentDidMount() {\n    petfinder.pet\n      .get({\n        output: \"full\",\n        id: this.props.id\n      })\n      .then(data => {\n        const pet = data.petfinder.pet;\n        let breed;\n        if (Array.isArray(pet.breeds.breed)) {\n          breed = pet.breeds.breed.join(\", \");\n        } else {\n          breed = pet.breeds.breed;\n        }\n        this.setState({\n          name: pet.name,\n          animal: pet.animal,\n          location: `${pet.contact.city}, ${pet.contact.state}`,\n          decription: pet.description,\n          media: pet.media,\n          breed,\n          loading: false\n        });\n      })\n      .catch(() => {\n        navigate(\"/\");\n      });\n  }\n  render() {\n    if (this.state.loading) {\n      return <h1>loading</h1>;\n    }\n    const { name, animal, breed, location, description, media } = this.state;\n\n    return (\n      <div className=\"details\">\n        <Carousel media={media} />\n        <h1>{name}</h1>\n        <h2>\n          {animal}-{breed} -{location}\n        </h2>\n        <p>{description}</p>\n      </div>\n    );\n  }\n}\n\nexport default Details;\n"},"lineCount":null}},"error":null,"hash":"6fda5f52663bb1b7c88ef5ecdcd5